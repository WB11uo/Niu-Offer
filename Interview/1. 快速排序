public void quickSort(int[] arr, int low, int high) {
	if (low >= high) return;
	int i = low;
	int j = high;
	int p = arr[low];
	int temp;
	
	while(i < j) {
		while (arr[j] >= p && i < j) {
			j--;
		}
		
		while (arr[i] <= p && i < j) {
			i++;
		}
		
		temp = arr[i];
		arr[i] = arr[j];
		arr[j] = temp;
	}
	arr[low] = arr[i];
	arr[i] = p;
	quickSort(arr, low, j - 1);
	quickSort(arr, j + 1, high);
}
